{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ansezz/nova-categories-field": "*",
        "ansezz/wp-importer": "*",
        "ebess/advanced-nova-media-library": "^2.1",
        "fideloper/proxy": "^4.0",
        "jameslkingsley/nova-stripe-theme": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/nova": "*",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0",
        "mll-lab/laravel-graphql-playground": "^0.3.3",
        "nuwave/lighthouse": "^2.6",
        "spatie/eloquent-sortable": "^3.4",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/nova-tags-field": "^1.1",
        "spatie/nova-translatable": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.4"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/nova-categories-field"
        },
        {
            "type": "path",
            "url": "./nova-components/WpImporter"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelVueJs\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
