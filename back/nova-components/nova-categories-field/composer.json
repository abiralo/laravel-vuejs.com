{
    "name": "ansezz/nova-categories-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ansezz\\CategoriesField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ansezz\\CategoriesField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
